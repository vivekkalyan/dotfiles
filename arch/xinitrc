#xinput --set-prop 8 154 1.4,0,0,0,1.4,0,0,0,1

# Keyboard
keyboard

# Set keyboard delay and rate
xset r rate 300 50

# redshift
redshift -l 1.27:103.84 &

# picom
picom -b

# set background
background

# dunst (notifications)
dunst &

# nextcloud
nextcloud &

# zotero
zotero &

# Statusbar
while true; do
   xsetroot -name "$( date +" %d/%m/%y %R" )"
   sleep 20s    # Update time at interval
done &

exec dwm
